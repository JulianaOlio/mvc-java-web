<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>projetoLogin</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  
 <servlet>
  	<servlet-name>ProjetoLogin</servlet-name>
  	<servlet-class>ProjetoLogin</servlet-class>
 </servlet>
  
 <servlet-mapping>
  	<servlet-name>ProjetoLogin</servlet-name>
  	<url-pattern>/login</url-pattern>
 </servlet-mapping>
 
  <servlet>
  	<servlet-name>MenuInicial</servlet-name>
  	<servlet-class>MenuInicial</servlet-class>
 </servlet>
  
 <servlet-mapping>
  	<servlet-name>>MenuInicial</servlet-name>
  	<url-pattern>/inicioMenu</url-pattern>
 </servlet-mapping>
 
   <servlet>
  	<servlet-name>ErroLogin</servlet-name>
  	<servlet-class>ErroLogin</servlet-class>
 </servlet>
  
 <servlet-mapping>
  	<servlet-name>>ErroLogin</servlet-name>
  	<url-pattern>/erroLogin</url-pattern>
 </servlet-mapping>
  
  
</web-app>